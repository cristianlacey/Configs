[user]
	name = Cristian Lacey
	email = clacey@princeton.edu
[core]
	editor = emacs
[color]
	ui = auto
[log]
	abbrevCommit = true
[format]
	pretty = oneline
[alias]
	lg = log -n 15 --oneline --decorate --graph --pretty=format:'%C(auto,191)%h%C(auto,191)%d %C(auto,63 bold)<%aN> %C(161 bold)(%cr) %C(reset)%s' --date=format:'%a %X %m/%d/%y'
	st = status
	ci = commit -m
